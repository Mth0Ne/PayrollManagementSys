<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard - Mazer Admin Dashboard</title>



    <link rel="shortcut icon" href="~/dist/assets/compiled/svg/favicon.svg" type="image/x-icon">
    <link rel="shortcut icon" href="data:image/png;base64," type="image/png">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    @*--datatables--*@
    

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

    <link rel="stylesheet" href="~/dist/assets/css/bootstrap.css">

    <link rel="stylesheet" href="~/dist/assets/compiled/css/app.css">
    <link rel="stylesheet" href="~/dist/assets/compiled/css/app-dark.css">
    <link rel="stylesheet" href="~/dist/assets/compiled/css/iconly.css">
</head>

<body>
    <script src="~/dist/assets/static/js/initTheme.js"></script>
    <div id="app">
        <div id="sidebar">
            <div class="sidebar-wrapper active">
                <div class="sidebar-header position-relative">
                    <div class="d-flex justify-content-between align-items-center">
                        <div class="logo">
                            <h3>ONUR SOMUNCU</h3>
                            <h3>ALPEREN TOKER</h3>
                        </div>
                    </div>
                </div>
                <div class="sidebar-menu">
                    <ul class="menu">
                        <li class="sidebar-title">Menu</li>
                        @if (!User.IsInRole("User"))
                        {
                            <li class="sidebar-item active ">
                                <a href="/Dashboard/Index" class='sidebar-link'>
                                    <i class="bi bi-grid-fill"></i>
                                    <span>Dashboard</span>
                                </a>


                            </li>
                        }

                       
                        @if (!User.IsInRole("User"))
                        {
                            <li class="sidebar-item">
                                <a href="/Employee/Employees" class='sidebar-link'>
                                    <i class="fa-solid fa-person"></i>
                                    <span>Çalışanlar</span>
                                </a>


                            </li>
                        }
                       
                        @if (!User.IsInRole("User"))
                        {
                            <li class="sidebar-item">
                                <a href="/Deparman/DepartmanPage" class='sidebar-link'>
                                    <i class='fas fa-landmark'></i>
                                    <span>Departmanlar</span>
                                </a>

                            </li>
                        }

                        
                        @if (!User.IsInRole("User"))
                        {
                            <li class="sidebar-item">
                                <a href="/Salary/EmployeSalary" class='sidebar-link'>
                                    <i class="fa-solid fa-money-bill"></i>
                                    <span>Maaş İşlemleri</span>
                                </a>

                            </li>
                        }

                     
                        @if (!User.IsInRole("User"))
                        {
                            <li class="sidebar-item ">
                                <a href="/Payroll/PayrollEmployee" class='sidebar-link'>
                                    <i class="fa-solid fa-file-invoice"></i>
                                    <span>Bordro</span>
                                </a>
                            </li>
                        }
                        @if (User.IsInRole("User"))
                        {
                            <li class="sidebar-item">
                                <a href="/Payroll/GetEmployeePayroll" class='sidebar-link'>
                                    <i class="fa-solid fa-file-invoice"></i>
                                    <span>Bordro Al</span>
                                </a>
                            </li>
                            
                        }

                        <li class="sidebar-item" style="position: absolute; bottom: 0;">
                            <a href="/Auth/Logout" class='sidebar-link'>
                                <i class="bi bi-box-arrow-left"></i>
                                <span>Çıkış Yap</span>
                            </a>
                        </li>


                    </ul>
                </div>
            </div>
        </div>
        <div id="main" style="background-color:whitesmoke">
            <header class="mb-3">
                <a href="#" class="burger-btn d-block d-xl-none">
                    <i class="bi bi-justify fs-3"></i>
                </a>
            </header>

            <div class="page-content" style="background-color">
                @RenderBody()
            </div>
        </div>
    </div>
   @* <script src=" ~/dist/assets/static/js/components/dark.js"></script>
    <script src="~/dist/assets/extensions/perfect-scrollbar/perfect-scrollbar.min.js"></script>*@


    <script src="~/dist/assets/compiled/js/app.js"></script>



    <!-- Need: Apexcharts -->
    <script src="~/dist/assets/extensions/apexcharts/apexcharts.min.js"></script>
    <script src="~/dist/assets/static/js/pages/dashboard.js"></script>


    <!-- Google Charts-->
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

    @RenderSection("Scripts", required: false)

</body>

</html>